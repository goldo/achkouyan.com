{
  "version": 2,
  "regions": ["sfo", "bru"],
  "alias": [
    "achkouyan.now.sh",
    "www.achkouyan.com",
    "franck.achkouyan.com",
    "achkouyan.com"
  ],
  "builds": [
    { "src": "/static/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "s-maxage=86400" }
    }
  ]
}
