{
  "version": 2,
  "alias": [
    "achkouyan.now.sh",
    "www.achkouyan.com",
    "franck.achkouyan.com",
    "achkouyan.com",
  ],
  "functions": {
    "pages/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "^/(favicon.ico|robots.txt|sitemap.xml)$",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0" }
    }
  ]
}
