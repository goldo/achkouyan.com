{
  "version": 2,
  "regions": ["all"],
  "alias": [
    "achkouyan.now.sh",
    "www.achkouyan.com",
    "franck.achkouyan.com",
    "achkouyan.com",
    "franck.i-s.dev"
  ],
  "functions": {
    "*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "^/(favicon.ico|robots.txt|sitemap.xml)$",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0" },
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "s-maxage=31536000, max-age=0" }
    }
  ]
}
